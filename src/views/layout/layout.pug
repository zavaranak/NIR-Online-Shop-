doctype html
head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    link(rel='stylesheet', href='/css/layout.css')    
    script(src="/script/script.js")
    script(src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js") 
body 
    .navbar
        .logo
            img(class='logoImg', src='/img/logo.png', alt='brandLogo')
        div
            a(href='/home') 
                h1 Brand Name 
        .navbar-items
            a(href='/products') 
                b Products
            a(href='/news') 
                b News
            a(href='/about-us') 
                b About Us
            a#logIn(href='/myprofile') 
                b My Profile
            a#cartbox(href='/myprofile/cart')
                b Cart 
    block content
    title= title


